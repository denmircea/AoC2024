#include <iostream>
#include <fstream>
#include <algorithm>
#include <vector>
#include <map>
#include <set>
#include <unordered_map>
#include<algorithm>
#include <string>
#include <math.h>

using namespace std;

ifstream fin("test.in");
ofstream fout("test.out");
vector<pair<pair<long long, long long>, pair<long long, long long>>> robots;
long long px, py, vx, vy;

long long n = 103;
long long m = 101;
//long long n = 7;
//long long m = 11;
set<pair<long long, long long>> visited;
int dx[] = { -1,0,1,0,-1,1,1,-1 };
int dy[] = { 0,1,0,-1,1,1,-1,-1 };

void dfs(int x,int y, vector<vector<char>>& mat) {
	visited.insert({ x,y });
	for (int dir = 0;dir < 8;dir++) {
		int nx = x + dx[dir];
		int ny = y + dy[dir];
		if (nx >= 0 && nx < n && ny >= 0 && ny < m && mat[nx][ny] == '#' && visited.find({ nx,ny }) == visited.end()) {
			dfs(nx, ny, mat);
		}
	}
}

void printmat(vector<pair<pair<long long, long long>, pair<long long, long long>>> robots) {
	vector<vector<char>> mat(n, vector<char>(m, '.'));
	for (auto it : robots) {
		mat[it.first.first][it.first.second] = '#';
	}
	for (long long i = 0;i < n;i++) {
		for (long long j = 0;j < m;j++) {
			fout << mat[i][j];
		}
		fout << endl;
	}
	fout << endl;

}
int main() {
	while (fin >> px >> py >> vx >> vy) {
		swap(px, py);
		swap(vx, vy);
		robots.push_back({ {px, py}, {vx, vy} });
	}
	set<vector<string>> s;
	for (long long i = 0;i < 8170;i++) {
		vector<string> cs;
		for (auto it : robots) {
			cs.push_back(to_string(it.first.first) + "_" + to_string(it.first.second));
		}
		s.insert(cs);
		if (s.size() != i + 1) {
			cout << i;
			break;
		}
		for (auto& it : robots) {
			it.first.first += it.second.first;
			it.first.second += it.second.second;
			it.first.first = (it.first.first + 19*n) % n;
			it.first.second = (it.first.second + 19 * m) % m;
		}
		if (i > 8165) {
			fout << i << endl;
			printmat(robots);
		}
		
	}
	double linx = n/2;
	double liny = m/2;
	vector<long long> res(4, 0);
	for (auto it : robots) {
		if (it.first.first < linx && it.first.second < liny) {
			res[0]++;
		}
		else if (it.first.first < linx && it.first.second > liny) {
			res[1]++;
		}
		else if (it.first.first > linx && it.first.second < liny) {
			res[2]++;
		}
		else if(it.first.first > linx && it.first.second>liny) {
			res[3]++;
		}
	}
	cout << res[0] * res[1] * res[2] * res[3];
	return 0;
}

